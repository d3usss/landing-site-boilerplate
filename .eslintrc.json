{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env" : {
      "browser" : true,
      "node" : true,
      "es6" : true
  },
  "rules" : {
      "no-unused-vars" : 2,
      "no-undef" : 2,
      "no-console": "off"
  },
  "parserOptions": {
      "sourceType": "module"
  }
}
